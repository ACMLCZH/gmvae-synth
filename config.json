{
    "name": "gmvae-synth",
    "n_gpu": 1,
    "arch": {
        "type": "CnnGMVAE",
        "args": {
            "input_size": [
                256,
                43
            ],
            "latent_dim": 16,
            "n_class": 12,
            "is_pitch_condition": true,
            "is_pitch_discriminate": true,
            "logvar_trainable": false,
            "pow_exp": 0,
            "is_featExtract": false
        }
    },
    "data_loader": {
        "type": "NormDataLoader",
        "args": {
            "data_dir": "data/spec-norm",
            "batch_size": 128,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 0
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.0001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss_recon": {
        "type": "MSEloss",
        "args": {
            "weight": 1,
            "effect_epoch": 1
        }
    },
    "loss_latent": {
        "type": "KLlatent",
        "args": {
            "weight": 1,
            "effect_epoch": 1
        }
    },
    "loss_class": {
        "type": "KLclass",
        "args": {
            "weight": 1,
            "effect_epoch": 1
        }
    },
    "loss_class_label": {
        "type": "CEloss",
        "args": {
            "weight": 1,
            "effect_epoch": 1
        }
    },
    "loss_pitch_discriminate": {
        "type": "PDloss",
        "args": {
            "weight": 1,
            "effect_epoch": 1
        }
    },
    "loss_pitch_emb": {
        "type": "KLpitch",
        "args": {
            "weight": 1,
            "effect_epoch": 1
        }
    },
    "metrics": [
        "test_acc",
        "classify_acc"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 500,
            "gamma": 0.1
        }
    },
    "trainer": {
        "label_portion": 1.0,
        "use_mfcc": false,
        "use_flux": false,
        "epochs": 10000,
        "save_dir": "saved",
        "save_period": 10000,
        "verbosity": 1,
        "monitor": "max val_lower_bound",
        "early_stop": 1000,
        "tensorboardX": true,
        "log_dir": "saved/runs"
    }
}
